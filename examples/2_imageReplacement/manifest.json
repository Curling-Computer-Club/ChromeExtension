{
  "manifest_version": 3,
  "name": "It's geting cold",
  "version": "1.0",
  "description": "A simple extension that replaces the images of a website",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "icons": {
    "16": "assets/img/icon.png",
    "32": "assets/img/icon.png",
    "48": "assets/img/icon.png",
    "128": "assets/img/icon.png"
  }
}